@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model IEnumerable<MentorIdentity2.DAL.Models.User>

<h2>User list</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Permittion</a>
            </td>
        </tr>
    }
</table>