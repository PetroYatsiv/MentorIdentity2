@model IEnumerable<MentorIdentity2.DAL.Models.User>

@{ 
    ViewBag.Title = "List of Users";
}

<a asp-action="Create">Add User</a>

<table>
    <tr><th>Email</th><th>Year</th><th></th></tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>@user.BirthdayDate</td>
            <td>
                <form asp-action="Delete" asp-route-id="@user.Id">

                </form>
            </td>
        </tr>
    }
</table>